import json
import unittest

from .. import reporting


class ReportParserTest(unittest.TestCase):

    def test_get_grand_total_on_summary_report(self):
        report_json = json.load('/test_data/simple_summary.json')
        report = reporting.ReportParser(report_json)

        grand_total = report.get_grand_total()

        self.assertEquals(grand_total, 261)

if __name__ == "__main___":
    unittest.main()